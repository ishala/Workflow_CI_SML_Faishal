name: mobile-price-classification

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      dataset: {type: str, default: "cleaned_data.csv"}
      ncomps: {type: int, default: 3}
      ntrials: {type: int, default: 30}
    command: "python modelling.py --dataset {dataset} --ncomps {ncomps} --ntrials {ntrials}"